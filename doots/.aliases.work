alias cshopify='code ~/src/github.com/Shopify/shopify'
alias cshipify='code ~/src/github.com/Shopify/shipify'
alias cweb='code ~/src/github.com/Shopify/web'
alias clabelcat='code ~/src/github.com/Shopify/labelcat'
alias cbloodhound='code ~/src/github.com/Shopify/bloodhound'
